<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qianlima.offline.mapper.TestLcMapper">

  <select id="getLcList" resultType="com.qianlima.offline.bean.LcVo">
      SELECT * FROM t_table
  </select>

  <update id="uptestLc">
      UPDATE t_table SET status=1 where id =#{id}
  </update>

    <select id="getMapForOne" resultType="map">
        SELECT * FROM t_table where project_id = #{projectId} AND user_id =#{userId}
    </select>
</mapper>